{
  "name": "reed",
  "author": "61soldiers",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Chill epub reader",
  "private": true,
  "homepage": "./",
  "main": "./electron/main.js",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "command-exists": "^1.2.9",
    "concurrently": "^5.3.0",
    "custom-electron-titlebar": "^3.2.5",
    "electron-is-dev": "^1.2.0",
    "epubjs": "^0.3.73",
    "file-loader": "^6.2.0",
    "forerunnerdb": "^2.0.22",
    "material-ui-color": "^0.4.6",
    "material-ui-popup-state": "^1.7.1",
    "node-ebook-converter": "^1.0.9",
    "notistack": "^1.0.3",
    "pdf-extraction": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^17.0.1",
    "react-dropdown": "^1.9.0",
    "react-reader": "^0.20.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "wait-on": "^5.2.1"
  },
  "devDependencies": {
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "ebuild": "electron-builder",
    "dist": "npm run build && npm run ebuild",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.evolvedmesh.reed",
    "productName": "Reed",
    "extends": null,
    "directories": {
      "output": "out"
    },
    "files": [
      "*.js",
      "*.css",
      "*.fdb",
      "build",
      "public",
      "data",
      "node_modules",
      "electron"
    ],
    "extraFiles": [
      "data/*.fdb"
    ],
    "win": {
      "target": "nsis",
      "icon": "./src/Resources/EvolvedMesh LOGO [Transparent] dark mode.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./src/Resources/EvolvedMesh LOGO [Transparent] dark mode.ico",
      "uninstallerIcon": "./src/Resources/EvolvedMesh LOGO [Transparent] dark mode.ico",
      "createDesktopShortcut": true,
      "shortcutName": "Reed ${version}"
    }
  }
}
